{
  "name": "image-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- sst bind next dev",
    "lint": "next lint",
    "start": "next start",
    "drizzle:generate": "drizzle-kit generate:pg --out drizzle/migrations --schema drizzle/schema.ts",
    "drizzle:migrate": "npx dotenv -e .env -- node drizzle/migrate.mjs",
    "next:dev": "next dev",
    "sst:dev": "npx dotenv -e .env -v SKIP_ENV_VALIDATION=true -- npx sst dev",
    "sst:deploy": "npx dotenv -e .env.production -v SKIP_ENV_VALIDATION=true -- npx sst deploy --stage prod",
    "sst:remove": "npx dotenv -e .env.production -v SKIP_ENV_VALIDATION=true -- npx sst remove"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.363.0",
    "@clerk/nextjs": "^4.21.14",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@upstash/redis": "^1.22.0",
    "drizzle-orm": "^0.27.0",
    "jotai": "^2.2.2",
    "nanoid": "^4.0.2",
    "next": "^13.4.2",
    "openai": "^3.3.0",
    "pg": "^8.11.1",
    "postgres": "^3.3.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "superjson": "1.12.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.0",
    "@types/pg": "^8.10.2",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "^10.4.14",
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.1.156",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.3",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "sst": "^2.19.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  }
}
